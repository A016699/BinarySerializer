<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Description>A declarative serialization framework for controlling formatting of data at the byte level.</Description>
    <AssemblyName>BinarySerializer</AssemblyName>
    <PackageTags>Serialization;Serializer;Binary;Format;Protocol</PackageTags>
    <PackageReleaseNotes>CRCs and other value fields are now checked during deserialization and a nested InvalidDataException will be thrown if the actual value does not match the computed value.  To suppress this behavior, specify a BindingMode of OneWayToSource.  This release also includes a fix to deserialization events reporting on fields with value attributes, which would occasionally report an incorrect value.</PackageReleaseNotes>
    <PackageProjectUrl>http://binaryserializer.net</PackageProjectUrl>
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>http://binaryserializer.net</RepositoryUrl>
    <AssemblyVersion>7.9.0.0</AssemblyVersion>
    <FileVersion>7.9.0.0</FileVersion>
    <Version>7.9</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Jeff Haynes</Authors>
    <Company />
    <RootNamespace>BinarySerialization</RootNamespace>
    <PackageIconUrl>https://raw.githubusercontent.com/jefffhaynes/BinarySerializer/master/BinarySerializer.Docs/bs.png</PackageIconUrl>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
    <DebugType>portable</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <DefineConstants>TRACE;DEBUG;NETSTANDARD1_3; TESTASYNC</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Collections" Version="4.3.0" />
    <PackageReference Include="System.IO" Version="4.3.0" />
    <PackageReference Include="System.Linq" Version="4.3.0" />
    <PackageReference Include="System.Linq.Expressions" Version="4.3.0" />
    <PackageReference Include="System.Reflection" Version="4.3.0" />
    <PackageReference Include="System.Reflection.Extensions" Version="4.3.0" />
    <PackageReference Include="System.Reflection.TypeExtensions" Version="4.4.0" />
    <PackageReference Include="System.Resources.ResourceManager" Version="4.3.0" />
    <PackageReference Include="System.Runtime" Version="4.3.0" />
    <PackageReference Include="System.Runtime.Extensions" Version="4.3.0" />
    <PackageReference Include="System.Text.Encoding" Version="4.3.0" />
    <PackageReference Include="System.Threading" Version="4.3.0" />
  </ItemGroup>
</Project>